                    xmlns:vm="clr-namespace:WpfApp1.ViewModels"
                    xmlns:views="clr-namespace:WpfApp1.Views" >
    
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <views:HomeView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <views:SettingsView/>
    </DataTemplate>
    
</ResourceDictionary>